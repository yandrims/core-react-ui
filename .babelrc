{
	"plugins": [
		[
			"styled-components",
			{ "ssr": true, "displayName": true, "preprocess": false }
		],
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		],
		[
			"@babel/plugin-proposal-optional-chaining",
			{
				"loose": false
			}
		],
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": false
			}
		]
	],
	"presets": ["@babel/env", "@babel/react", "@babel/flow"]
}
